<script>


    // add 'swiper-container' class to .elementor-container
    jQuery( '.do-monthly-pricing-swiper > .elementor-container' ).addClass( 'six' );
    jQuery( '.do-monthly-pricing-swiper > .elementor-container' ).addClass( 'swiper-container' );
    // add 'swiper-wrapper' class to .elementor-row
    jQuery( '.do-monthly-pricing-swiper .swiper-container > .elementor-row' ).addClass( 'swiper-wrapper' );
    // add 'swiper-slide' class to each column
    jQuery( '.do-monthly-pricing-swiper .swiper-wrapper > div' ).addClass( 'swiper-slide' );
	jQuery('.six').append('<div class="swiper-pagination"></div><div class="swiper-button-prev"></div><div class="swiper-button-next"></div>'); 
    
    
    var swiper = new Swiper('.six', {
      loop: true, 
	  slidesPerView: 1,
      parallax: true,
      speed: 500, 
      observer: true,
      observeParents: true,
		navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
          },
    });
	
	jQuery(document).ready(function(){
			var boxWidth = 750;
			var animateHeightSales = 600;
			var animateHeightDemo = 600;
			var animateHeightFeedback = 600;
			var animateHeightReport = 545;
			var curHeight = jQuery(".main-custy").height();
// 			var heightAuto = jQuery(".main-custy").css('height', 'auto');
			jQuery(".sales-content").css('width', '0px');
			jQuery("#sales-btn-1").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: 867});
					jQuery(".sales-content").animate({
						width: 320,
						opacity: 1
					});
				} else {
					jQuery(".main-custy").animate({height: animateHeightSales});
					jQuery(".sales-content").animate({
						width: boxWidth,
						opacity: 1
					});
				}
				
			});
			jQuery(".close-div").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: curHeight});
				} else {
					jQuery(".main-custy").animate({height: curHeight});
				}
				jQuery(".sales-content").animate({
					width: 0,
					opacity: 0
				});
			});

			// .......................... //

			jQuery("#demo-btn-2").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: 880});
					jQuery(".demo-content").animate({
						width: 320,
						opacity: 1
					});
				} else {
					jQuery(".main-custy").animate({height: animateHeightDemo});
					jQuery(".demo-content").animate({
						width: boxWidth,
						opacity: 1
					});
				}
			});
			jQuery(".close-div").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: curHeight});
				} else {
					jQuery(".main-custy").animate({height: curHeight});
				}
				jQuery(".demo-content").animate({
					width: 0,
					opacity: 0
				});
			});

			// .......................... //

			jQuery("#feedback-btn-3").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: 875});
					jQuery(".feedback-content").animate({
						width: 320,
						opacity: 1
					});
				} else {
					jQuery(".main-custy").animate({height: animateHeightFeedback});
					jQuery(".feedback-content").animate({
						width: boxWidth,
						opacity: 1
					});
				}
			});
			jQuery(".close-div").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: curHeight});
				} else {
					jQuery(".main-custy").animate({height: curHeight});
				}
				jQuery(".feedback-content").animate({
					width: 0,
					opacity: 0
				});
			});

			// .......................... //

			jQuery("#abuse-btn-4").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: 766});
					jQuery(".abuse-content").animate({
						width: 320,
						opacity: 1
					});
				} else {
					jQuery(".main-custy").animate({height: animateHeightReport});
					jQuery(".abuse-content").animate({
						width: boxWidth,
						opacity: 1
					});
				}
			});
			jQuery(".close-div").click(function(){
				if (window.matchMedia('(max-width: 480px)').matches) {
					jQuery(".main-custy").animate({height: curHeight});
				} else {
					jQuery(".main-custy").animate({height: curHeight});
				}
				jQuery(".abuse-content").animate({
					width: 0,
					opacity: 0
				});
			});
		});
		var acc = document.getElementsByClassName("ss-accordion");
		var i;

		for (i = 0; i < acc.length; i++) {
		  acc[i].addEventListener("click", function() {
			this.classList.toggle("ss-active");
			var panel = this.nextElementSibling;
			if (panel.style.display === "block") {
			  panel.style.display = "none";
			} else {
			  panel.style.display = "block";
			}
		  });
		}
</script>
