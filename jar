	<script>
    // add 'swiper-container' class to .elementor-container
    jQuery( '.testimonial-swiper > .elementor-container' ).addClass( 'swiper-container' );
    // add 'swiper-wrapper' class to .elementor-row
    jQuery( '.testimonial-swiper .swiper-container > .elementor-row' ).addClass( 'swiper-wrapper' );
    // add 'swiper-slide' class to each column
    jQuery( '.testimonial-swiper .swiper-wrapper > div' ).addClass( 'swiper-slide' );
    // pagination
    jQuery( '.testimonial-swiper .swiper-container' ).append( '<div class="swiper-pagination"></div>' );
    // previous & next 
    jQuery( '.testimonial-swiper .swiper-container' ).append( '<div class="swiper-button-next"></div>' ).append( '<div class="swiper-button-prev"></div>' );
    var swiper = new Swiper('.testimonial-swiper > .elementor-container', {
      loop: true, 
	  autoplay: false,
	  slidesPerView: 1,
      parallax: true,
      speed: 500, 
      observer: true,
      observeParents: true,
	  pagination: {
        el: '.testimonial-swiper .swiper-container .swiper-pagination', 
        type: 'bullets', 
        clickable: true, 
      },
	  navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },		
	
    });

</script>
		
 
<script>


    // add 'swiper-container' class to .elementor-container
    jQuery( '.aws-hourly-pricing-swiper > .elementor-container' ).addClass( 'three' );
    jQuery( '.aws-hourly-pricing-swiper > .elementor-container' ).addClass( 'swiper-container' );
    // add 'swiper-wrapper' class to .elementor-row
    jQuery( '.aws-hourly-pricing-swiper .swiper-container > .elementor-row' ).addClass( 'swiper-wrapper' );
    // add 'swiper-slide' class to each column
    jQuery( '.aws-hourly-pricing-swiper .swiper-wrapper > div' ).addClass( 'swiper-slide' );
	jQuery('.three').append('<div class="swiper-pagination"></div><div class="swiper-button-prev"></div><div class="swiper-button-next"></div>'); 
    
    
    var swiper = new Swiper('.three', {
      loop: true, 
	  slidesPerView: 1,
      parallax: true,
      speed: 500, 
      observer: true,
      observeParents: true,
		navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
          },
    });
	
	

</script>
		
 
